<!DOCTYPE html>
<html>
<head>
  <title>Medfiles</title>
	<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
	<%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
	<%= javascript_include_tag "autocomplete-rails.js" %>

	<%= csrf_meta_tags %>
</head>
<body>

<p class="notice"><%= notice %></p>
<p class="alert"><%= alert %></p>



<% if admin_signed_in? %>
	<%= link_to("ADMIN PANEL", admin_panel_path) %>
	<%= link_to("LOG_OUT", destroy_admin_session_path) %>
<% end %>

<div id="navbar">

	<% if doctor_signed_in? %>
		<%= link_to("PROFILE", current_doctor) %>
		<%= link_to("LOG OUT", destroy_doctor_session_path) %>
	<% else %>
		<%= link_to("SIGN IN", new_doctor_session_path) %>
		<%= link_to("SIGN UP", new_doctor_registration_path) %>
	<% end %>
</div>

<%= yield %>
<%= javascript_include_tag "autocomplete-rails.js" %>
</body>
</html>
